{
    "name": "ShadyChars/Bibcite",
    "description": "A simple Bibtex parser and CSL citation generator.",
    "autoload": {
        "psr-4": {
            "Bibcite\\Main\\": "main",
            "Bibcite\\Common\\": "common",
            "Bibcite\\Admin\\": "admin"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "citation-style-language/locales",
                "version":"1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/citation-style-language/locales.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "citation-style-language/styles-distribution",
                "version":"1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/citation-style-language/styles-distribution.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/ShadyChars/converter"
        }
    ],
    "require": {
        "php":">=7.1.0",
        "citation-style-language/locales": "@dev",
        "citation-style-language/styles-distribution": "@dev",
        "seboettg/citeproc-php": "^2",
        "renanbr/bibtex-parser": "^2",
        "geissler/converter": "dev-master",
        "apache/log4php": "2.3.0",
		"cocur/slugify": "^3.1",
		"twig/twig": "^2.4",
        "psr/log": "^1.0"
    }
}
