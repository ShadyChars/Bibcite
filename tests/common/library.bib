@article{10.2307/41157700,
abstract = {This essay examines the contradictions in the racial thought of the renowned Austrian anthropologist and ethnologist Felix von Luschan (1854-1924). In the 1880s, he began comparing the physical, mental, and linguistic characteristics of peoples worldwide, arguing that "race" generally was a meaningless construct. Luschan challenged racial theories that supported anti-Semitism and demolished racial hierarchies of "superior" and "inferior" peoples. Despite his anti-racist anthropology, at the First Universal Races Congress in 1911 Luschan simultaneously espoused racial tolerance and supported colonialism, eugenics, and German nationalism. The African-American historian, sociologist, and polemicist W.E.B. Du Bois first noted the inconsistencies in Luschan's arguments. Stranded in the United States in the early months of World War I, Luschan tried to refine his conclusions by recording the racial characteristics of hundreds of African-American subjects. Based on his American field work, Luschan wrote optimistically about the progress of American blacks since emancipation, yet, like white conservatives in America, he judged African Americans to be the crux of an unsolvable "race problem." Though committed theoretically to trans-Atlantic racial reform, gradually after 1900 Luschan's understanding of "race" became enshrouded by hereditarianism and Social Darwinism. Ironically, while his early dismissal of Aryan supremacy and anti-Semitic theories earned Luschan the condemnation of Nazis in the 1930s, his later endorsement of eugenics and Social Darwinism left Luschan undervalued and marginalized by modern students of racial thought.},
author = {Smith, John David},
issn = {03402827},
journal = {Amerikastudien / American Studies},
number = {1},
pages = {23--38},
publisher = {Universit{\"{a}}tsverlag WINTER Gmbh},
title = {{W.E.B. Du Bois, Felix von Luschan, and Racial Reform at the Fin de Si{\`{e}}cle}},
url = {http://www.jstor.org/stable/41157700},
volume = {47},
year = {2002}
}
