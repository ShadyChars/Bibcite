@article{murphy1995hyphens-marginal,
annote = {(private-note)"The Marginal Hyphen or Coupling Stroke" (299-305) - derived from the sublinear hyphen. Stage 1: sublinear hyphen. Stage 2: where a hyphenated word pair occurred at the end of a line, a sublinear hyphen would be placed at the end of the first line or the start of the second. See Paris A. of Plato. Vat. gr. 1591, copied in Italy 964-965. Some boat-shaped hyphens (8r, 15r), some horizontal (292r). In the 12th C, paired sublinear hyphens appear at the end of the first line *and* the start of the second. See 123v of Vat. gr. 1992, Gregory of Nazianzus, 1104. Stage 3: boat-shaped sublinear hyphens protrude into the margin. See New York Pierpont Morgan 652, 890, 294v. Stage 4: stages 2 & 3 suggested to scribes the possibility of using hyphens for non-compound words, where the page layout would otherwise cause confusion. Stage 5: true "coupling stroke"; any divided word warranted a hyphen. First example is Ambr. B 56 sup., copied in Campania 1022. A copyist mistakenly separated a single word and corrected it with a marginal hyphen. Practice became used for *any* word by 12th C. Can discern the creation of a true coupling stroke because words which did not normally warrant a sublinear hyphen were hyphenated at line ends.},
author = {Murphy, D J},
journal = {Greek, Roman, and Byzantine Studies},
keywords = {hyphen,shady_characters},
number = {3},
pages = {299--305},
title = {{Hyphens in Greek Manuscripts}},
volume = {36},
year = {1995}
}
