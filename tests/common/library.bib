month = {nov},
number = {11},
pages = {2083--2096},
publisher = {Institute of Electrical and Electronics Engineers},
shorttitle = {Bell versus Gray},
title = {{The Bell versus Gray telephone dispute: Resolving a 144-year-old controversy}},
volume = {108},
year = {2020}
}
