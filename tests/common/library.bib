@article{Pawlik2011,
abstract = {Excavation of the Micoquian site Inden-Altdorf (Weisweiler-124) near the former German capital Bonn in western Germany has revealed the first valid open-site habitation features with hut-like structures and associated hearths for the Middle Palaeolithic in Central Europe. It has been dated to the Eemian interglacial (OIS 5e), a warm interglacial between 128 and 115 ka BP. Various wear traces and especially organic residues have been detected on a large number of stone tools using microscopic use-wear analysis of lithics recovered from the site. A multi-level analysis developed through an experimental framework and archaeological study using optical light microscopes, scanning electron microscopes and energy-dispersive X-ray microprobes identified the adhering residues as birch pitch. Birch pitch is the oldest synthetically produced material and was used as an adhesive to attach lithic implements to wooden shafts. While such hafting technology is commonly associated with modern humans in the Upper Palaeolithic, the birch pitch residues found on the Micoquian tools of Inden-Altdorf suggest that hafting technologies and the frequent use of multi-component tools already existed in the Middle Palaeolithic, c. 120 ka. BP in central Europe. {\textcopyright} 2011 Elsevier Ltd.},
author = {Pawlik, Alfred F. and Thissen, J{\"{u}}rgen P.},
doi = {10.1016/j.jas.2011.03.001},
issn = {10959238},
journal = {Journal of Archaeological Science},
keywords = {Behavioural modernity,Central Europe,Eemian,Hafting residues,Lithic technology,Micoquian,Micro-wear analysis},
month = {jul},
number = {7},
pages = {1699--1708},
publisher = {Academic Press},
shorttitle = {Hafted armatures at Inden-Altdorf},
title = {{Hafted armatures and multi-component tool design at the Micoquian site of Inden-Altdorf, Germany}},
volume = {38},
year = {2011}
}
