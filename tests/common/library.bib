@article{Soole1952,
abstract = {The danger to persons exposed to nuclear radiations from fission products deposited after an atomic explosion cannot be fully estimated from a measurement of the ionization dose-rate at any particular time. This is because the intensity of ionization from fission products decreases continually, and the rate of decrease depends on the age of the fission products. Thus the total dose received by anyone is less than would be expected if allowance is not made for the decay of dose-rate during the period of exposure. A slide-rule is described by means of which this decay can be allowed for quickly and accurately. By its use, all problems of external radiation dose received from decaying fission products may be rapidly solved.},
author = {Soole, B. W.},
doi = {10.1088/0950-7671/29/6/308},
journal = {Journal of Scientific Instruments},
month = {jun},
number = {6},
pages = {189--192},
publisher = {IOP Publishing},
shorttitle = {RADIAC slide rule},
title = {{The RADIAC slide rule for the computation of external radiation dose from nuclear fission products}},
url = {https://doi.org/10.1088%2F0950-7671%2F29%2F6%2F308},
volume = {29},
year = {1952}
}
